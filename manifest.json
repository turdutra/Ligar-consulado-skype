{
  "manifest_version": 2,
  "name": "Ligar pro consulado",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "flag.png"
  },
  "permissions": [
      "tabs",
      "activeTab",
      "http://*/*",
      "https://*/*"
  ]
}
